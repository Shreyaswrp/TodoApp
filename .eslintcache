({\n        usernameErrMsg: \"\",\n        passwordErrMsg: \"\",\n      });\n    }\n\n    this.setState({\n      username: \"\",\n      password: \"\",\n    });\n  };\n\n  inputHandler = (e) => {\n    this.setState({\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  usererrMsgHandler = () => {\n    this.setState({\n      usernameErrMsg: \"\",\n    });\n  };\n  psswderrMsgHandler = () => {\n    this.setState({\n      passwordErrMsg: \"\",\n    });\n  };\n\n  changeType = () => {\n    this.setState({\n      passwordtype: !this.state.passwordtype,\n    });\n  };\n\n  loginHandler = async () => {\n    const user = {\n      user_email: this.state.username,\n      password: this.state.password,\n    };\n\n    const resp = axios\n      .post(\"http://api.ganies.com/login\", user)\n      .then((res) => {\n        console.log(res.data);\n        if (res.status === 200) {\n          return this.props.history.push(\"/Todo\");\n        } else {\n          console.log(res.error);\n        }\n      })\n      .catch((err) => {\n        console.log(\"comming\");\n        console.log(err.response);\n      });\n  };\n\n  render() {\n    return (\n      <div className=\"main-container\">\n        <form\n          className=\"userLogin-Form\"\n          autoComplete=\"off\"\n          onSubmit={this.formSubmitHandler}\n        >\n          <header className=\"account-Heading\">Login to your Account</header>\n          <div className=\"username-Wrapper\">\n            <input\n              placeholder=\" \"\n              className=\"inputField-Username\"\n              name=\"username\"\n              value={this.state.username}\n              onChange={this.inputHandler}\n              onClick={this.usererrMsgHandler}\n            ></input>\n            <label className=\"username-text\">Username or Email</label>\n          </div>\n          <span className=\"errorMsg\">{this.state.usernameErrMsg}</span>\n\n          <div className=\"password-Wrapper\">\n            <input\n              type={this.state.passwordtype ? \"password\" : \"text\"}\n              name=\"password\"\n              value={this.state.password}\n              placeholder=\" \"\n              className=\"inputField-Password\"\n              onChange={this.inputHandler}\n              onClick={this.psswderrMsgHandler}\n            ></input>\n            <label className=\"password-text\">Password</label>\n            <i\n              class={\n                this.state.passwordtype ? \"fas fa-eye-slash\" : \"far fa-eye\"\n              }\n              onClick={this.changeType}\n            ></i>\n          </div>\n\n          <span className=\"errorMsg\">{this.state.passwordErrMsg}</span>\n          <button className=\"submit-Button\" type=\"submit\">\n            LogIn\n          </button>\n          <a className=\"newAccount-Link\"> Create a account</a>\n        </form>\n        <div className=\"rightImage-Container \">\n          <img className=\"account-Image\" src={accountSvg}></img>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Login;\n","/home/swastik/Desktop/todo/src/App.js",[],"/home/swastik/Desktop/todo/src/Components/Loginpage/Login.js",["52","53","54"],"/home/swastik/Desktop/todo/src/Components/todoapp/Itemslist.js",[],"/home/swastik/Desktop/todo/src/Components/todoapp/Todo.js",["55"],"import React, {Component} from \"react\";\nimport {confirmAlert} from \"react-confirm-alert\";\nimport \"react-confirm-alert/src/react-confirm-alert.css\";\nimport Itemslist from \"./Itemslist\";\nimport \"../../Styles/Todo.css\";\n\nclass Todo extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            list: [],\n            errorMessage: \"\",\n            editStatus: false,\n            currentElement: {\n                text: \"\",\n                key: \"\"\n            }\n        };\n    }\n\n    submitHandler = (e) => {\n        e.preventDefault();\n        const currentdata = this.state.currentElement;\n        if (currentdata.text === \"\" || currentdata.text.trim().length === 0) {\n            this.setState({errorMessage: \"Task cannot be empty\"});\n        } else {\n            const inputdata = [\n                currentdata, ...this.state.list\n            ];\n            this.setState({\n                list: inputdata,\n                errorMessage: \"\",\n                currentElement: {\n                    text: \"\",\n                    key: \"\"\n                }\n            });\n        }\n    };\n\n    removeMsgHandler = () => {\n        this.setState({errorMessage: \"\"});\n    };\n\n    inputHandler = (e) => {\n        this.setState({\n            currentElement: {\n                text: e.target.value,\n                key: Date.now()\n            }\n        });\n    };\n\n    deleteItem = (key) => {\n        confirmAlert({\n            title: \"Confirm to delete\",\n            message: \"Are you sure to do this.\",\n            buttons: [\n                {\n                    label: \"Yes\",\n                    onClick: () => {\n                        const newList = this\n                            .state\n                            .list\n                            .filter((item) => item.key !== key);\n                        this.setState({list: newList});\n                    }\n                }, {\n                    label: \"No\",\n                    onClick: () => console.log(\"No clicked\")\n                }\n            ]\n        });\n    };\n\n    editItem = (textone, key) => {\n        if (this.state.editStatus === true) {\n            const newTodo = this.state.list;\n            newTodo.map((item) => {\n                if (item.key === key) {\n                    item.text = textone;\n                }\n            });\n            this.setState({list: newTodo});\n        } else {\n            console.log(\"nothing\");\n        }\n    };\n\n    changeValueHanlder = () => {\n        this.setState({\n            editStatus: !this.state.editStatus\n        });\n    };\n\n    logoutHandler = () => {\n        return this\n            .props\n            .history\n            .push(\"/\");\n    };\n\n    render() {\n        return (\n            <div className=\"todo-container\">\n                <form className=\"todo-form\" autoComplete=\"off\" onSubmit={this.submitHandler}>\n                    <h2 className=\"heading\">ToDo List</h2>\n                    <input\n                        className=\"task-input-field\"\n                        placeholder=\"enter your task\"\n                        name=\"task\"\n                        value={this.state.currentElement.text}\n                        onChange={this.inputHandler}\n                        onClick={this.removeMsgHandler}></input>\n                    <button className=\"addtask-button\" type=\"submit\">+</button>\n                    <div className=\"error-Message\">{this.state.errorMessage}</div>\n                    <Itemslist\n                        list={this.state.list}\n                        deleteItem={this.deleteItem}\n                        editItem={this.editItem}\n                        changeValue={this.changeValueHanlder}></Itemslist>\n                </form>\n                <button class=\"logout-button\" onClick={this.logoutHandler}>Logout</button>\n            </div>\n        );\n    }\n}\n\nexport default Todo;\n","/home/swastik/Desktop/todo/src/index.js",[],{"ruleId":"56","severity":1,"message":"57","line":24,"column":26,"nodeType":"58","messageId":"59","endLine":24,"endColumn":28},{"ruleId":"60","severity":1,"message":"61","line":77,"column":26,"nodeType":"62","messageId":"63","endLine":77,"endColumn":28},{"ruleId":"64","severity":1,"message":"65","line":20,"column":30,"nodeType":"66","messageId":"67","endLine":20,"endColumn":31,"suggestions":"68"},{"ruleId":"64","severity":1,"message":"65","line":20,"column":55,"nodeType":"66","messageId":"67","endLine":20,"endColumn":56,"suggestions":"6